{
  "manifest_version": 2,

  "name": "Popser!",
  "description": "Popser! Your serials book!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "js": ["includeStylesheets.js", "bundle.js"]
    }
  ]
}
